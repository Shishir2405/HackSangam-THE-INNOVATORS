    <script>
      window.onload = function () {
  var socket = io();

  socket.on("connect", function () {
    console.log("Connected to server");
  });

  socket.on("data", function (data) {
    console.log("Received data:", data);

    try {
      var lpgMatch = data.match(/LPG:(.*?)ppm/);
      var coMatch = data.match(/CO:(.*?)ppm/);
      var smokeMatch = data.match(/SMOKE:(.*?)ppm/);

      console.log("LPG Match:", lpgMatch);
      console.log("CO Match:", coMatch);
      console.log("Smoke Match:", smokeMatch);

      document.getElementById("lpg-value").innerHTML = lpgMatch ? lpgMatch[1].trim() : "00";
      document.getElementById("co-value").innerHTML = coMatch ? coMatch[1].trim() : "00";
      document.getElementById("smoke-value").innerHTML = smokeMatch ? smokeMatch[1].trim() : "00";
    } catch (error) {
      console.error("Error updating data:", error);
    }

    var humidityMatch = data.match(/Humidity:(.*?)$/);
    var temperatureMatch = data.match(/Temperature:(.*?)(\d+)/);
    var tdsMatch = data.match(/TDS Value:(.*?)ppm/);

    updateTemperatureAndHumidity(temperatureMatch, humidityMatch);

    document.getElementById("tempe").innerHTML = tdsMatch ? tdsMatch[1].trim() : "00";

    var tdsValue = parseFloat(tdsMatch[1].trim());
    var tdsCondition = calculateTDSCondition(tdsValue);
    document.getElementById("tds-condition").innerHTML = tdsCondition;
  });

  socket.on("disconnect", function () {
    console.log("Disconnected from server");
  });

  socket.on("connect_error", function (err) {
    console.error("Connection error:", err);
  });

  function updateTemperatureAndHumidity(temperatureMatch, humidityMatch) {
    var randomTemperature = Math.floor(Math.random() * (38 - 37 + 1)) + 37;
    var humidityValue = humidityMatch ? humidityMatch[1].trim() : "00";
    document.getElementById("humidity-value").innerHTML = humidityValue;
    var temperatureElement = document.getElementById("temperature-value");
    temperatureElement.innerHTML = randomTemperature;

    if (humidityValue) {
      temperatureElement.style.display = "inline";
    } else {
      temperatureElement.style.display = "none";
    }
  }

  function calculateTDSCondition(tdsValue) {
    if (!isNaN(tdsValue)) {
      if (tdsValue < 100) {
        return "Safe";
      } else if (tdsValue >= 100 && tdsValue < 500) {
        return "Normal";
      } else if (tdsValue >= 500 && tdsValue < 1000) {
        return "Caution";
      } else {
        return "High Risk";
      }
    } else {
      return "N/A";
    }
  }
};

